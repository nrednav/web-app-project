<div class="login_form">
	<h1><u>Login</u></h1>

	<%= form_tag sessions_path do %>

		<%= label_tag :email, "Email:" %> <br>
		<%= text_field_tag :email, params[:email] %>
		<br>
		<%= label_tag :password, "Password:" %> <br>
		<%= password_field_tag :password %>
		<br>
		<%= submit_tag "Login" %>

	<% end %> <br>
</div>
